<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@drawable/loginbackfround"

    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/dim_m"
        android:layout_marginEnd="@dimen/dim_m">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:id="@+id/relative"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dim_2xl"
                android:layout_marginTop="@dimen/dim_3xl"
                android:layout_marginEnd="@dimen/dim_2xl"
                android:layoutDirection="rtl">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/ic_person_black_24dp"
                    android:backgroundTint="@color/Black" />

                <com.example.sofra.helper.MultiSelectionSpinner
                    android:id="@+id/FragmentSellerRegisterCategory"
                    android:layout_width="match_parent"
                    android:padding="@dimen/dim_l"
                    android:layout_height="@dimen/dim_4xl">

                </com.example.sofra.helper.MultiSelectionSpinner>
            </RelativeLayout>



            <EditText
                android:id="@+id/Fragment_sale_User_Cahe_money1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_4xl"
                android:layout_below="@id/relative"
                android:layout_alignParentStart="true"
                android:backgroundTint="@color/white"
                android:drawableEnd="@drawable/ic_attach_money_black_24dp"
                android:drawablePadding="@dimen/dim_m"
                android:hint="الحد الادني للطلب "
                android:inputType="phone"
                android:lines="1"
                android:layout_marginStart="@dimen/dim_2xl"
                android:layout_marginTop="@dimen/dim_xl"
                android:layout_marginEnd="@dimen/dim_2xl"
                android:maxLines="1"
                android:textSize="@dimen/txt_size_l"
                android:textStyle="bold"
               />

            <EditText
                android:id="@+id/Fragment_sale_User_delivery_money"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_4xl"
                android:layout_below="@id/Fragment_sale_User_Cahe_money1"
                android:layout_marginStart="@dimen/dim_2xl"
                android:layout_marginTop="@dimen/dim_xl"
                android:layout_marginEnd="@dimen/dim_2xl"
                android:backgroundTint="@color/white"
                android:drawableEnd="@drawable/ic_attach_money_black_24dp"
                android:drawablePadding="@dimen/dim_m"
                android:hint=" رسوم التوصيل  "
                android:inputType="phone"
                android:lines="1"
                android:maxLines="1"
                android:textSize="@dimen/txt_size_l"
                android:textStyle="bold"
                android:layout_marginBottom="@dimen/dim_l">

            </EditText>

            <Switch
                android:id="@+id/registerMoreRestaurantSwitchState"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Fragment_sale_User_delivery_money"
                android:layout_alignParentEnd="true"
                android:layout_margin="@dimen/dim_m"
                android:text="state"

                />


            <TextView
                android:id="@+id/Txt_thre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/registerMoreRestaurantSwitchState"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="@dimen/dim_l"
                android:layout_marginEnd="@dimen/dim_2xl"
                android:text=" بيانات التوصيل "
                android:textColor="@color/Black"
                android:textSize="@dimen/txt_size_xl" />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/Fragment_sale_User_number_of_phone"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/Txt_thre"
                android:layout_marginStart="@dimen/dim_2xl"
                android:layout_marginTop="@dimen/dim_2xl"
                android:layout_marginEnd="@dimen/dim_2xl">

                <android.support.design.widget.TextInputEditText

                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dim_4xl"
                    android:backgroundTint="@color/white"
                    android:drawableEnd="@drawable/phone"
                    android:drawablePadding="@dimen/dim_m"
                    android:hint=" الجوال"
                    android:inputType="phone"
                    android:lines="1"
                    android:maxLines="1"
                    android:textSize="@dimen/txt_size_l"
                    android:textStyle="bold" />
            </android.support.design.widget.TextInputLayout>

            <EditText
                android:id="@+id/Fragment_sale_Whatsup"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_4xl"
                android:layout_below="@id/Fragment_sale_User_number_of_phone"
                android:layout_marginStart="@dimen/dim_2xl"
                android:layout_marginTop="@dimen/dim_xl"
                android:backgroundTint="@color/white"
                android:drawableEnd="@drawable/whatsapp1"
                android:drawablePadding="@dimen/dim_m"
                android:hint="واتس اب "
                android:inputType="phone"
                android:lines="1"
                android:maxLines="1"
                android:textSize="@dimen/txt_size_l"
                android:textStyle="bold"
                android:layout_marginEnd="@dimen/dim_2xl">

        </EditText>

            <!--<TextView-->
                <!--android:id="@+id/id_thro"-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_below="@id/Fragment_sale_Whatsup"-->
                <!--android:layout_alignParentStart="true"-->
                <!--android:layout_marginTop="@dimen/dim_l"-->
                <!--android:layout_marginEnd="@dimen/dim_2xl"-->
                <!--android:text=" صورة المتجر"-->
                <!--android:textColor="@color/Black"-->
                <!--android:textSize="@dimen/txt_size_xl" />-->

            <!--<ImageView-->
                <!--android:id="@+id/Fragment_sale_User_Image_Matgar"-->
                <!--android:layout_width="100dp"-->
                <!--android:layout_height="100dp"-->
                <!--android:layout_below="@id/id_thro"-->
                <!--android:layout_alignParentStart="true"-->
                <!--android:layout_marginTop="@dimen/dim_m"-->
                <!--android:layout_marginEnd="@dimen/dim_xl"-->
                <!--android:background="@drawable/shapephoto"-->
                <!--android:src="@drawable/ic_store_mall_directory_black_24dp" />-->

            <Button
                android:id="@+id/Fragment_sale_User_Register"
                android:layout_width="220dp"
                android:layout_height="40dp"
                android:layout_below="@id/Fragment_sale_Whatsup"
                android:layout_centerInParent="true"
                android:background="@drawable/shapebutton"
                android:drawableStart="@drawable/enter"
                android:text=" تسجيل الدخول "
                android:textColor="@color/white" />
        </RelativeLayout>
    </ScrollView>

</RelativeLayout>